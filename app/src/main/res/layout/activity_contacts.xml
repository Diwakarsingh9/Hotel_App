<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:geekui="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wyndhamgarden.app.Contacts"
    android:orientation="vertical">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/contact_bg">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <views.TypefaceTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Contact Us"
                android:id="@+id/textView13"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:textColor="#ffffff"
                android:textSize="35dp"
                geekui:customTypeface="fonts/JosefinSans-SemiBold.ttf" />

            <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:id="@+id/back"
                android:layout_gravity="left|center_vertical"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:src="@drawable/back" />
        </FrameLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/imageView8"
                android:src="@drawable/contactus"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp" />
        </LinearLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/scroll">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <views.TypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="DETAILS"
                        android:id="@+id/textView14"
                        android:textSize="25dp"
                        geekui:customTypeface="fonts/JosefinSans-SemiBold.ttf"
                        android:textColor="#FFFFFF"
                        android:layout_marginLeft="20dp"
                        android:layout_marginBottom="15dp"
                        android:layout_marginRight="10dp" />

                    <views.TypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TELEPHONE : 01400 250909"
                        android:id="@+id/textView15"
                        android:layout_marginLeft="20dp"
                        android:textSize="22dp"
                        geekui:customTypeface="fonts/JosefinSans-SemiBold.ttf"
                        android:textColor="#FFFFFF"
                        android:layout_marginBottom="5dp"
                        android:layout_marginRight="10dp" />

                    <views.TypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="OVERSEAS : + 441400 250909"
                        android:id="@+id/textView1522"
                        android:layout_marginLeft="20dp"
                        android:textSize="22dp"
                        geekui:customTypeface="fonts/JosefinSans-SemiBold.ttf"
                        android:textColor="#FFFFFF"
                        android:layout_marginBottom="5dp"
                        android:layout_marginRight="10dp" />

                    <views.TypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="FAX : 01400 250130"
                        android:id="@+id/textView1533"
                        android:layout_marginLeft="20dp"
                        android:textSize="22dp"
                        geekui:customTypeface="fonts/JosefinSans-SemiBold.ttf"
                        android:textColor="#FFFFFF"
                        android:layout_marginBottom="5dp"
                        android:layout_marginRight="10dp" />
                    <views.TypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="EMAIL ADDRESS : ab@wyndhamgardengrantham.co.uk"
                        android:id="@+id/textView1533dd"
                        android:layout_marginLeft="20dp"
                        android:textSize="22dp"
                        geekui:customTypeface="fonts/JosefinSans-SemiBold.ttf"
                        android:textColor="#FFFFFF"
                        android:layout_marginBottom="20dp"
                        android:layout_marginRight="10dp" />

                    <views.TypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="ADDRESS"
                        android:id="@+id/textView1455"
                        android:textSize="25dp"
                        geekui:customTypeface="fonts/JosefinSans-SemiBold.ttf"
                        android:textColor="#FFFFFF"
                        android:layout_marginLeft="20dp"
                        android:layout_marginBottom="15dp"
                        android:layout_marginRight="10dp" />

                    <views.TypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="TOLL BAR ROAD"
                        android:id="@+id/textView1544"
                        android:layout_marginLeft="20dp"
                        android:textSize="22dp"
                        geekui:customTypeface="fonts/JosefinSans-SemiBold.ttf"
                        android:textColor="#FFFFFF"
                        android:layout_marginBottom="5dp"
                        android:layout_marginRight="10dp" />

                    <views.TypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="MARSTON"
                        android:id="@+id/textView152233"
                        android:layout_marginLeft="20dp"
                        android:textSize="22dp"
                        geekui:customTypeface="fonts/JosefinSans-SemiBold.ttf"
                        android:textColor="#FFFFFF"
                        android:layout_marginBottom="5dp"
                        android:layout_marginRight="10dp" />

                    <views.TypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="LINCOLNSHIRE"
                        android:id="@+id/textView1533EE"
                        android:layout_marginLeft="20dp"
                        android:textSize="22dp"
                        geekui:customTypeface="fonts/JosefinSans-SemiBold.ttf"
                        android:textColor="#FFFFFF"
                        android:layout_marginBottom="5dp"
                        android:layout_marginRight="10dp" />
                    <views.TypefaceTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="NG32 2HT"
                        android:id="@+id/textView1533ERRE"
                        android:layout_marginLeft="20dp"
                        android:textSize="22dp"
                        geekui:customTypeface="fonts/JosefinSans-SemiBold.ttf"
                        android:textColor="#FFFFFF"
                        android:layout_marginBottom="20dp"
                        android:layout_marginRight="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/backgroundcontcts"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginBottom="5dp"
                        android:padding="5dp">

                        <EditText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/namesss"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:hint="NAME"
                            android:singleLine="true"
                            android:textColor="#ffffff"
                            android:textColorHint="#ffffff"
                            android:background="@android:color/transparent"
                            android:textSize="25dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/backgroundcontcts"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginBottom="5dp"
                        android:padding="5dp">

                        <EditText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/emailsss"
                            android:layout_marginLeft="10dp"
                            android:hint="EMAIL ADDRESS"
                            android:textColor="#ffffff"
                            android:singleLine="true"
                            android:textColorHint="#ffffff"
                            android:background="@android:color/transparent"
                            android:textSize="25dp"
                            android:inputType="textWebEmailAddress"
                            android:layout_weight="1" />

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:id="@+id/emailvalidat"
                            android:layout_margin="5dp"
                            android:src="@drawable/cross" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/backgroundcontcts"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_marginBottom="10dp"
                        android:padding="5dp">

                        <EditText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/enquiryss"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:hint="ENQUIRY"
                            android:textColor="#ffffff"
                            android:textColorHint="#ffffff"
                            android:background="@android:color/transparent"
                            android:textSize="25dp" />
                    </LinearLayout>

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <views.TypefaceTextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="SUBMIT"
                            android:id="@+id/submit"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginBottom="10dp"
                            android:textColor="#FFFFFF"
                            android:textSize="30dp"
                            geekui:customTypeface="fonts/JosefinSans-SemiBold.ttf"
                            android:autoText="false"
                            android:background="@drawable/submitbg"
                            android:gravity="center"
                            android:padding="5dp"
                            android:layout_gravity="center" />

                        <ProgressBar
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/pb"
                            android:layout_gravity="center"
                            android:visibility="gone" />
                    </FrameLayout>

                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</LinearLayout>
